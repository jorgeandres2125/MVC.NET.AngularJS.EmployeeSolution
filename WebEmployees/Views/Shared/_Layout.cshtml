<!DOCTYPE html>
<html ng-app="app">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"]</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <footer class="border-top footer text-muted">
    </footer>
    <script>
        var currentHost = '@ViewData["CurrentHost"]';
    </script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/lib/angularjs/angular.min.js" asp-append-version="true"></script>
    <script src="~/lib/angularjs/angular-route.min.js" asp-append-version="true"></script>
    <script src="~/js/app/app.js" asp-append-version="true"></script>
    <script src="~/js/app/shared/services/dataService.js" asp-append-version="true"></script>
    <script src="~/js/app/controllers/employeeCtrl.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
